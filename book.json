{
    "title": "Terminus 后端开发实用手册",
    "description": "",
    "author": "crazybuglzy",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
    },
    "plugins": [
        "-lunr",
        "-highlight",
        "-livereload",
        "-search",
        "-sharing",
        "search-pro",
        "splitter",
        "expandable-chapters-small",
        "prism@^2.1.0",
        "prism-themes@^0.0.2",
        "anchors",
        "copy-code-button",
        "include-codeblock",
        "ace",
        "anchor-navigation-ex",
        "tbfed-pagefooter",
        "sectionx",
        "alerts",
        "multipart",
        "github@^2.0.0",
        "github-buttons@2.1.0",
        "donate",
        "ga"
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        },
         "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "anchor-navigation-ex": {
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right",
            "multipleH1": false,
            "associatedWithSummary":false
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy crazybuglzy.com 2018",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },"github": {
            "url": "https://github.com/crazyBugLzy/book-gitbook"
        },
        "github-buttons": {
            "repo": "crazyBugLzy/book-gitbook",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "donate": {
            "wechat": "http://crazybuglzy.top/resource/weixinpay.jpeg",
            "alipay": "http://crazybuglzy.top/resource/alipay.jpeg",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "ga":{
            "token":"UA-117514919-1"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "theme": "monokai"
        }
    }
}
